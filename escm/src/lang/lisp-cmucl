;; lisp - configurations for the Common Lisp language (GNU CLISP)
lisp -quiet -batch -eval '(setf *prompt* "")'
;; init
(let (escm_version escm_input_file
escm_interpreter escm_query_string)
;; nil
nil
;; bind
(setq @VAR@ @VAL@)
;; string
(princ "@STR@")
;; display
(princ @EXP@)
;; newline
(princ #\nl)
;; finish
(quit))
