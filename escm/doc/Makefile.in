$(ESCM) :
	cd ../src/ && make

doc : $(man_MANS) $(HTML_FILES)
	LANG=C && $(ESCM) -e "(define *pwd* \"`pwd`\")" -e "(define *date* \"`date -u`\")" readme.escm > readme.html
	 w3m -dump readme.html > ../README

.epod.1 :
	$(ESCM) -e "(define *pwd* \"`pwd`\")" $< > $*.pod
	$(POD2HTML) $*.pod > $*.html
	$(POD2MAN) -c "" -r "" $*.pod > $@

.escm.html :
	$(ESCM) -e "(define *pwd* \"`pwd`\")" $< > $@

clean-local:
	-rm *~
	-rm *.pod

realclean : clean-local
	-rm *.1
	-rm *.html

